@model Lib.Model.UploadMethodModel
@{
    ViewBag.Title = "Lab Stock API Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .ui-menu {
        width: 434.328px !important;
    }

        .ui-menu .ui-menu-item {
            width: 415.328px !important;
        }

    .autocomplete {
        position: relative;
        display: inline-block;
    }

    .g-head-t {
        font-size: 12px;
        padding: 0 12px;
        color: grey;
    }

    .g-amt-t {
        font-size: 15px;
        padding: 0 12px;
        color: #003d66;
        font-weight: 700;
    }

    .spn-Yes1 {
        border: 1px solid #e5e5e5;
        border-radius: 10px;
        padding: 0px 5px 0px 5px;
        font-size: 10px;
        font-weight: 600;
        background: #28a745;
        color: white;
    }

    .spn-No1 {
        border: 1px solid #e5e5e5;
        border-radius: 10px;
        padding: 0px 5px 0px 5px;
        font-size: 10px;
        font-weight: 600;
        background: #ef0808;
        color: white;
    }

    .disabled {
        cursor: not-allowed;
    }

    .btn-spn-opt:hover {
        background-color: #80808017;
        border: 1px solid #003d66;
    }

    .btn-spn-opt {
        border: 1px solid #e5e5e5;
        border-radius: 28px;
        padding: 5px 15px;
        float: left;
        margin-bottom: 5px;
        margin-right: 5px;
        transition-duration: 0.3s;
        -webkit-transition: 0.3s;
        cursor: pointer;
        font-size: 11px;
        white-space: nowrap;
        font-weight: 600;
        background: #fff;
        color: #003d66;
        position: relative;
        z-index: 0;
        overflow: hidden;
        margin-top: 5px;
    }

    .btn-spn-opt-active {
        color: #fff !important;
        background-color: #003d66 !important;
    }

    .pwd_field {
        -webkit-text-security: disc;
    }

    #txtCompanyName_suggestion {
        font-size: 11.5px;
    }

    [ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak, .ng-hide:not(.ng-hide-animate) {
        display: none !important;
    }

    .img-block {
        display: block;
    }

    .img-none {
        display: none;
    }

    ul {
        list-style: none;
        padding: 0;
        margin: 0;
    }

    .api-filter-popup ul.color-whit-box li {
        display: inline-block;
        margin: 4px 4px;
    }

    ul.color-whit-box li.active {
        background: rgba(0, 61, 102,1);
        color: #fff;
    }

    ul.color-whit-box li {
        padding: 3px 13px;
        -webkit-box-shadow: 0 0 2px rgba(0, 61, 102,0.3);
        -moz-box-shadow: 0 0 2px rgba(0, 61, 102,0.3);
        box-shadow: 0 0 2px rgba(0, 61, 102,0.3);
        margin: 2px 4px;
        -webkit-border-radius: 50px;
        -moz-border-radius: 50px;
        border-radius: 50px;
        cursor: pointer;
        font-size: 14px;
        color: #003d66;
        font-weight: 500;
        background: #fff;
        white-space: nowrap;
        -webkit-transition: all 0.3s;
        -moz-transition: all 0.3s;
        -ms-transition: all 0.3s;
        -o-transition: all 0.3s;
        transition: all 0.3s;
    }

    .custom-box-list p {
        word-break: break-all;
        border: 1px solid #ced4da;
        padding: 0px 4px;
        font-weight: 400;
        color: #495057;
        background-color: #fff;
        background-clip: padding-box;
        font-size: 13px;
        height: 35px;
        line-height: normal;
        border-radius: .25rem;
        transition: border-color .15s ease-in-out,box-shadow .15s ease-in-out;
        overflow: auto;
        margin: 0;
    }

    .Fi-Criteria {
        font-size: 13px;
    }

    .tabboxdiv > .row {
        border-bottom: none;
    }

    .allcrt {
        margin: 0px 4px 4px 0px;
        color: #003d66;
        font-weight: normal !important;
        padding: 5px 12px;
    }

    .fncy-rst {
        right: -6px;
        top: 13px;
        position: absolute;
        /*font-size: 14px;*/
        cursor: pointer;
    }

    .sym-sec .carat-dropdown-main ul li {
        display: inline-block;
        margin: -3px 0px 0px 0px;
        width: 21%;
    }

    .g-head-t {
        font-size: 12px;
        padding: 0 12px;
        color: grey;
    }

    .g-amt-t {
        font-size: 12px;
        padding: 0 12px;
        color: #003d66;
    }

    .f_clr_clk {
        margin-top: -11px;
        z-index: 9999;
        width: 109px;
    }

    .fltr-bx {
        word-break: break-all;
        line-height: 1.3;
        font-size: 11px !important;
        padding: 0.1rem 0.1rem;
        height: 2rem;
        border: 1px solid #CED4DA;
        font-weight: 400;
        border-radius: 0.375rem !important;
        color: black;
        overflow: auto;
    }

    .chebox-fill, .chebox-empty {
        cursor: pointer !important;
        width: 21px !important;
        margin-top: 0px !important;
        height: 21px !important;
        border-radius: 0% !important;
    }
    /*body.modal-open {
      height: 100vh;
      overflow-y: hidden;
    }*/
</style>
@Html.HiddenFor(x => x.iTransId)


<h4 class="card-title m-ttl">Lab Stock API Detail</h4>

<div class="row">
    <h5 class="card-title" style="margin-left: 15px; ">Credential Detail</h5>
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <div class="form-group row">
                    <div class="col">
                        <label>User Name <span style="color:red;">*</span></label>
                        @*<input type="text" class="form-control form-control-sm" placeholder="User Name" id="txtUserName" name="txtUserName">*@
                        @Html.TextBoxFor(x => x.UserName, new { @value = Model.UserName, @class = "form-control form-control-sm", @maxLength = "100", @autocomplete = "off" })
                    </div>
                    <div class="col">
                        <label>Password <span style="color:red;">*</span></label>
                        @*<input type="text" class="form-control form-control-sm" placeholder="Password" id="txtPassword" name="txtPassword">*@
                        @Html.TextBoxFor(x => x.Password, new { @value = Model.Password, @class = "form-control form-control-sm", @maxLength = "100", @autocomplete = "off" })
                    </div>
                    <div class="col">
                        <label>Export Type <span style="color:red;">*</span></label>
                        @Html.DropDownListFor(x => x.ExportType, Model.ExportTypeList, "Select", new { @class = "form-control form-control-sm", })
                        @*<select class="form-control form-control-sm" id="ddlExportType" name="ddlExportType">
                    <option value="">Select</option>
                    <option value="EXCEL(.xlsx)">EXCEL (.xlsx)</option>
                    <option value="EXCEL(.xls)">EXCEL (.xls)</option>
                    <option value="CSV">CSV</option>
                    <option value="XML">XML</option>
                    <option value="JSON">JSON</option>
                    <option value="JSON(Text)">JSON (text)</option>
                </select>*@
                    </div>
                    <div class="col" style="margin-right: -135px;">
                        <label>API Status</label>
                        <div class="form-check form-check-primary">
                            <label class="form-check-label">
                                <input type="checkbox" class="form-check-input" checked=@Model.APIStatus id="chkAPIStatus" name="chkAPIStatus">
                                Active
                            </label>
                        </div>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col">
                        <label>API Name <span style="color:red;">*</span></label>
                        @*<input class="form-control form-control-sm" type="text" placeholder="File Name" id="txtFileName" name="txtFileName">*@
                        @Html.TextBoxFor(x => x.APIName, new { @value = Model.APIName, @class = "form-control form-control-sm", @maxLength = "100", @autocomplete = "off" })
                    </div>
                    <div class="col">
                        <label>User <span style="color:red;">*</span></label>
                        <div id="grid-margin stretch-card">
                            @Html.DropDownListFor(x => x.For_iUserId, Model.For_iUserIds, "Select", new { @class = "js-example-basic-single w-100", onchange = "UserDDL()" })
                        </div>
                    </div>
                    <div class="col">
                        <label>Company Name</label>
                        <label class="form-control form-control-sm" id="lblCompanyName" name="lblCompanyName" style="line-height: 0.98rem;font-size: 0.750rem !important;font-weight: 700;padding: 0.10rem 0.20rem 0.10rem;">@Model.For_CompName</label>
                    </div>
                </div>
                @if (Model.APIUrl != null)
                {
                    <strong style="font-size: 12px; color:red;">Download Link : @Model.APIUrl</strong>
                }
            </div>
        </div>
    </div>
