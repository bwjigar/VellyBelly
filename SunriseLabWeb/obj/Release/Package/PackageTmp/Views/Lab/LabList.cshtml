
@{
    ViewBag.Title = "Lab Stock API List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section styles {
    <link href="~/Content/css/daterangepicker.css" rel="stylesheet" />
}
<style>
    .ag-paging-page-summary-panel {
        left: 10px;
        position: relative;
    }

    .ag-theme-balham .ag-cell {
        white-space: normal !important;
        line-height: inherit;
        overflow-y: auto;
    }

    .Yes {
        margin-bottom: 1px;
        color: white;
        background-color: #008000ab;
        padding: 2px 5px;
        border-radius: 16px;
        font-weight: bold;
    }

    .No {
        margin-bottom: 1px;
        color: white;
        background-color: #ff0000cf;
        padding: 2px 5px;
        border-radius: 16px;
        font-weight: bold;
    }
</style>

<div class="row">
    <div class="col-xl-2 col-lg-12 col-md-12 col-sm-12 col-12">
        <h4 class="card-title m-ttl">Lab Stock API List</h4>

    </div>
    <div class="col-md-10">
        <button type="button" class="btn btn-inverse-dark btn-fw btnLabStockUpload" onclick="LabStockUpload('WITHOUT_MACRO');" style="float:right;margin-top: -19px;margin-left: 5px;display:none;">Without Macro Upload</button>
        <button type="button" class="btn btn-inverse-dark btn-fw btnLabStockUpload" onclick="LabStockUpload('MACRO');" style="float:right;margin-top: -19px;margin-left: 15px;display:none;">Macro Upload</button>
        <label id="lblLabStockUploadMsg" style="float: right;margin-top: -12px;font-size: 13.5px;font-weight: 700;"></label>
    </div>
</div>

<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <div class="form-group row">
                    <div class="col-lg-2">
                        <label>From Date</label>
                        <input type="text" class="form-control form-control-sm" placeholder="From Date" id="txtFromDate" name="txtFromDate">

                    </div>
                    <div class="col-lg-2">
                        <label>To Date</label>
                        <input type="text" class="form-control form-control-sm" placeholder="To Date" id="txtToDate" name="txtToDate">
                    </div>
                    <div class="col-lg-3">
                        <label>Search</label>
                        <input type="text" class="form-control form-control-sm" placeholder="Search" id="txtSearch" name="txtSearch">
                    </div>
                    <div class="col-lg-5" id="divmyModalbtn" style="padding: 14px 0px 14px 20px;">
                        <button type="button" class="btn btn-inverse-success btn-fw" onclick="btnSearch();">Search</button>
                        @*<button type="button" class="btn btn-inverse-info btn-fw" onclick="ExcelGet();">Excel</button>*@
                        <button type="button" class="btn btn-inverse-dark btn-fw" onclick="Clear();">Reset</button>
                        <button type="button" class="btn btn-inverse-primary btn-fw" onclick="Back();">Add New</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-12 grid-margin stretch-card">
        <div style="width:100%;">
            <div class="aggrid-section viewcart">
                <div class="row">
                    <div class="col-xl-12">
                        <!---- =============== start ag-grid =============== ---->

                        <div id="myGrid" style="width: 100%;height:500px;" class="ag-theme-balham">

                        </div>

                        <!---- =============== end ag-grid ================= ----->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section scripts {
    <script src="~/Content/js/wow.min.js"></script>
    <script src="~/Content/js/moment.min.js"></script>
    <script src="~/Content/js/daterangepicker.min.js"></script>
    <script src="~/Content/js/ag-grid-enterprise.min.js"></script>
    <script src="~/Content/js/Pages/LabList.js?version=5.0"></script>
}